#version 330 core
layout(location = 0) in vec3 aposition;
layout(location = 1) in vec3 anormal;
layout(location = 2) in vec2 atexcoord;
out vec2 texcoord;
out vec3 nor;
uniform mat4 model;
uniform mat4 view;
uniform mat4 projection;
void main()
{
	texcoord = atexcoord;
	nor = anormal;
	gl_Position = projection*view*model*vec4(aposition, 1.0);
}