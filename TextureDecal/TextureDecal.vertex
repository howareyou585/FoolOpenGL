#version 430 core
layout(location = 0) in vec3 aPosition;

layout(location = 1) in vec4 aTexCoord;
uniform mat4 model;
uniform mat4 view;
uniform mat4 projection;
out vec2 texCoord1;
out vec2 texCoord2;
void main()
{
	texCoord1 = aTexCoord.xy;
	texCoord2 = aTexCoord.zw;
	gl_Position =projection*view*model* vec4(aPosition, 1.0);
}